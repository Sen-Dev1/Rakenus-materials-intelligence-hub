			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "ComplianceID",
				"type": "String"
			},
			{
				"name": "DocumentType",
				"type": "String"
			},
			{
				"name": "ReferenceID",
				"type": "String"
			},
			{
				"name": "Description",
				"type": "String"
			},
			{
				"name": "ComplianceDate",
				"type": "String"
			},
			{
				"name": "PlantID",
				"type": "String"
			}
		]
	}
}