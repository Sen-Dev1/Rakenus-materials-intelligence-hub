{
	"name": "ds_csv_parameterized",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_adls_datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FolderName": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
